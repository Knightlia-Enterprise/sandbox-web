{
  "name": "sandbox-web",
  "version": "0.0.1",
  "description": "The web client for the sandbox mode of Particle Chat.",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "build": "npm run check && vite build",
    "start": "vite dev --port 3000",
    "preview": "vite preview",
    "test": "vitest run tests",
    "test:watch": "vitest tests",
    "test:coverage": "vitest run --coverage tests",
    "e2e": "playwright test",
    "e2e:debug": "playwright test --debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Knightlia/sandbox-web.git"
  },
  "author": "Saeed Afzal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Knightlia/sandbox-web/issues"
  },
  "homepage": "https://github.com/Knightlia/sandbox-web#readme",
  "dependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.15.2",
    "svelte": "^3.58.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.3",
    "@testing-library/svelte": "^3.2.2",
    "@vitest/coverage-c8": "^0.30.1",
    "jsdom": "^21.1.1",
    "sass": "^1.62.0",
    "svelte-check": "^3.2.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.0"
  }
}
